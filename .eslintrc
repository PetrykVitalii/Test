{
  "extends": ["airbnb-typescript"],
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "@typescript-eslint/no-use-before-define": ["error", { "variables": false }],
    "linebreak-style": "off",
    "react/prop-types": "off",
    "react/no-array-index-key": "off"
  }
}
